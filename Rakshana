COUNTS OCCURENCE PROGRAM 

import java.util.Comparator; 
import java.util.Map; 
import java.util.stream.Collectors; 
public class ex2
{ 
  public static void main(String[] args)
{
String str = "Good Morning"; 
str=str.toLowerCase(); 
Map<Character, Long> CharacterCount = str.chars().mapToObj(ch-> (char) ch).collect(Collectors.groupingBy(ch -> ch, Collectors.counting())); 
System.out.println("Counts of the string");
CharacterCount.entrySet().forEach(System.out::println); 
} 
}
